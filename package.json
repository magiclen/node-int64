{
  "name": "int64-napi",
  "version": "2.0.0",
  "description": "Use Rust to compute 64-bit signed integers.",
  "main": "src/lib.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "mocha tests/**/*.test.js",
    "build": "cargo-cp-artifact -nc index.node -- cargo build --release --message-format=json-render-diagnostics",
    "install": "npm run build",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magiclen/node-int64.git"
  },
  "keywords": [
    "int64",
    "long",
    "nodejs"
  ],
  "author": "Magic Len <len@magiclen.org> (https://magiclen.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magiclen/node-int64/issues"
  },
  "homepage": "https://magiclen.org/node-js-int64/",
  "dependencies": {
    "@types/node": "^15.6.1",
    "cargo-cp-artifact": "^0.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "mocha": "^8.4.0"
  }
}
